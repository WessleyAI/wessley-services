# Application Environment
APP_ENV=dev
PORT=8080

# Authentication
SUPABASE_JWT_PUBLIC_KEY=your-jwt-public-key-here
REQUIRE_AUTH=true

# Storage
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here
SUPABASE_BUCKET=ingestions
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=wessley-ingestions

# Queue
REDIS_URL=redis://localhost:6379

# OCR Providers
TESSERACT_LANGS=eng
DEEPSEEK_API_URL=https://api.deepseek.com
DEEPSEEK_API_KEY=your-deepseek-api-key
MISTRAL_OCR_API_URL=https://api.mistral.ai
MISTRAL_API_KEY=your-mistral-api-key

# Datastores
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Telemetry
SENTRY_DSN=your-sentry-dsn-here
PROMETHEUS_PORT=9090

# Feature Flags
FEATURE_SCHEMATIC_PARSE=true
OCR_ENGINES=tesseract,deepseek
STORE_DEBUG_OVERLAY=true
MAX_PAGES=50

# MinIO (for local development)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin